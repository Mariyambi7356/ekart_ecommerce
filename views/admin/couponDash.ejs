  <!-- End Header -->

  <!-- ======= Sidebar ======= -->
  <aside id="sidebar" class="sidebar">

    <ul class="sidebar-nav" id="sidebar-nav">

      <li class="nav-item">
        <a class="nav-link " href="/admin/home">
          <i class="bi bi-grid"></i>
          <span>Home</span>
        </a>
      </li><!-- End Dashboard Nav -->

      <li class="nav-item">
        <a class="nav-link " data-bs-target="#components-nav" href="/admin/dashboard">
          <i class="bi bi-menu-button-wide"></i><span>User</span><i class="bi  ms-auto"></i>
        </a>
      </li>


      <li class="nav-item">
        <a class="nav-link " data-bs-target="#components-nav" href="/admin/product-dashboard">
          <i class="bi bi-menu-button-wide"></i><span>Product</span><i class="bi  ms-auto"></i>
        </a>
      </li>


      <li class="nav-item">
        <a class="nav-link " data-bs-target="#components-nav">
          <i class="bi bi-menu-button-wide"></i><span>Coupon</span><i class="bi  ms-auto"></i>
        </a>
      </li>

      <li class="nav-item">
        <a class="nav-link " data-bs-target="#components-nav" href="/admin/brand-dashboard">
          <i class="bi bi-menu-button-wide"></i><span>Brand </span><i class="bi  ms-auto"></i>
        </a>
      </li>

      <li class="nav-item">
        <a class="nav-link " data-bs-target="#components-nav" href="/admin/category-dashboard">
          <i class="bi bi-menu-button-wide"></i><span>Category</span><i class="bi  ms-auto"></i>
        </a>
      </li>
      <li class="nav-item">
        <a class="nav-link " href="/admin/order-dashboard" data-bs-target="#components-nav">
          <i class="bi bi-menu-button-wide"></i><span>Orders</span><i class="bi  ms-auto"></i>
        </a>
      </li>
      <li class="nav-item">
        <a class="nav-link " href="/admin/banner-dashboard" data-bs-target="#components-nav">
          <i class="bi bi-menu-button-wide"></i><span>Banner</span><i class="bi  ms-auto"></i>
        </a>
      </li>


    </ul>
    </ul>


  </aside>
  <!-- End Sidebar-->

  <main id="main" class="main">
    <div class="pagetitle">
      <h1>Coupon Tables</h1>
      <nav>
        <ol class="breadcrumb">
          <li class="breadcrumb-item active">Data</li>
          <li class="breadcrumb-item">Coupon</li>

        </ol>
      </nav>
    </div>
    <!-- End Page Title -->

    <section class="section">
      <div class="row">
        <div class="col-lg-12">
          <div class="card">
            <div class="card-body">
              <!-- Table with stripped rows -->
              <table class="table datatable">
                <thead>
                  <tr>
                    <th style="font-family: 'EB Garamond', serif" scope="col">
                      Name
                    </th>
                    <th style="font-family: 'EB Garamond', serif" scope="col">
                      Code
                    </th>

                    <th style="font-family: 'EB Garamond', serif" scope="col">
                      StartDate
                    </th>
                    <th style="font-family: 'EB Garamond', serif" scope="col">
                      EndDate
                    </th>
                    <th style="font-family: 'EB Garamond', serif" scope="col">
                      maxDiscount
                    </th>
                    <th style="font-family: 'EB Garamond', serif" scope="col">
                      minPurchase
                    </th>
                    <th style="font-family: 'EB Garamond', serif" scope="col">
                      Status
                    </th>

                    <th style="font-family: 'EB Garamond', serif" scope="col">
                      Action
                    </th>

                  </tr>
                </thead>

                <tbody>
                  <% if (coupon.length> 0) { coupon.forEach(function(coupon)
                    { %>
                    <tr style="font-family: 'Oxygen', sans-serif">
                      <td>
                        <%= coupon.couponName %>
                      </td>
                      <td>
                        <%= coupon.couponCode %>
                      </td>
                      <td>
                        <%= coupon.startDate.toDateString() %>
                      </td>
                      <td>
                        <%= coupon.endDate.toDateString() %>
                      </td>
                      <td>
                        <%= coupon.maxDiscount %>
                      </td>
                      <td>
                        <%= coupon.minAmount %>
                      </td>
                      <td>
                        <% if (coupon.status===0) { %> Active <% } else if (coupon.status===1) { %> Inactive <% } %>
                      </td>
                      <td>

                        <a href="/admin/edit-coupon?id=<%= coupon._id %>" class="btn btn-success btn-sm mt-1">Edit</a>
                        <a href="/admin/delete-coupon?id=<%= coupon._id %>"
                          class="btn btn-danger btn-sm mt-1">Delete</a>

                        <% if (coupon.status===0) { %>
                          <a href="/admin/blockCoupon?id=<%= coupon._id %>"
                            class="btn btn-danger btn-sm mt-1">Unlist</a>
                          <% } else { %>
                            <a href="/admin/unBlockCoupon?id=<%= coupon._id %>"
                              class="btn btn-success btn-sm mt-1">List</a>
                            <% } %>
                      </td>
                    </tr>
                    <% }); } else { %>
                      <tr>
                        <td colspan="4">Coupon not found</td>
                      </tr>
                      <% } %>
                </tbody>
              </table>
              <% if(typeof message !=='undefined' ){ %>

                <p style="
                    font-size: small;
                    color: red;
                    font-family: 'Tilt Warp', cursive;
                  " class="ms-3">
                  <%= message %>
                </p>

                <% } %>

                  <!-- End Table with stripped rows -->
            </div>
          </div>
        </div>
      </div>
    </section>
  </main>
  <!-- End #main -->

  <a href="#" class="back-to-top d-flex align-items-center justify-content-center"><i
      class="bi bi-arrow-up-short"></i></a>

  <!-- Vendor JS Files -->
  <script src="assets/vendor/apexcharts/apexcharts.min.js"></script>
  <script src="assets/vendor/bootstrap/js/bootstrap.bundle.min.js"></script>
